{
  "name": "Dev.to Clipboard Images",
  "version": "1.0",
  "description": "Allows you to easily paste images from your clipboard into a dev.to article",
    "background": {
  "scripts": ["background.js"]},
  "page_action" :
  {
    "default_icon" : "icon-19.png",
    "default_title" : "Dev.to Clipboard Images"
  },
"permissions": ["tabs", "http://dev.to/", "https://dev.to/"],
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "manifest_version":2
}
